            function edit(div) {
                div.style.border = "3px solid #333";
                div.style.padding = "10px";
                div.contentEditable = true;
                //durum = false;
            }
            function save(div) {
                div.style.border = "";
                div.style.padding = "";
                div.contentEditable = false;
                //durum = true;
            }</script>
    }
